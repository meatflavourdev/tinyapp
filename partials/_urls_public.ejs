<section class="py-2">
  <div class="container">
    <h2 class="py-2">Public URLs</h2>
    <div class="card">
      <table class="table">
        <thead class="thead-dark">
          <tr>
            <th scope="col">Short URL</th>
            <th scope="col">Long URL</th>
          </tr>
        </thead>
        <tbody>
          <% for(let url of urls) { %>
          <tr>
            <td><a href="/u/<%= url.shortID %>"><%= url.shortID %></a></td>
            <td><%= url.longURL %></td>
          </tr>
          <% } %>
        </tbody>
      </table>
    </div>
  </div>
</section>
